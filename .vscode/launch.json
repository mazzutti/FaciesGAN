{
  "version": "0.2.0",
  "configurations": [
    {
      "name": "Python: Launch FaciesGAN (debugpy)",
      "type": "debugpy",
      "request": "launch",
      "program": "${workspaceFolder}/main.py",
      "console": "integratedTerminal",
      "cwd": "${workspaceFolder}",
      "justMyCode": false,
      "subProcess": true,
      "redirectOutput": true,
      "envFile": "${workspaceFolder}/.env",
      "args": [
        "--input-path",
        "data/",
        "--output-path",
        "results/",
        "--num-train-pyramids",
        "3",
        "--num-iter",
        "4000",
      ]
    }
  ]
}
